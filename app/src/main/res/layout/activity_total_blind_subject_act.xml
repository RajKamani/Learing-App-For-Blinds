<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <Button
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#2c8665"
        android:text="English"
        android:textSize="25dp"
        android:textColor="#d2c5c5"
        android:enabled="false"
        android:id="@+id/btnTBeng"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#59307c"
        android:text="Mathematics"
        android:textSize="25dp"
        android:textColor="#d2c5c5"
        android:enabled="false"
        android:id="@+id/btnTBmath"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#7e2433"
        android:text="History"
        android:textSize="25dp"
        android:textColor="#d2c5c5"
        android:enabled="false"
        android:id="@+id/btnTBhis"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#947642"
        android:text="General Knowledge"
        android:textSize="25dp"
        android:textColor="#d2c5c5"
        android:enabled="false"
        android:id="@+id/btnTBGK"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#375f80"
        android:text="Internet Search"
        android:textSize="25dp"
        android:textColor="#d2c5c5"
        android:enabled="false"
        android:id="@+id/btnTBinternet"
        />

</LinearLayout>